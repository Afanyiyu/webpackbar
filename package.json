{
  "name": "webpackbar",
  "version": "5.0.0-3",
  "description": "Elegant ProgressBar and Profiler for Webpack",
  "repository": "unjs/webpackbar",
  "license": "MIT",
  "main": "./dist/index.mjs",
  "exports": {
    "import": "./dist/index.mjs"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "webpack-cli --config demo/webpack.config.js",
    "lint": "eslint --ext .ts,.mjs",
    "prepack": "unbuild",
    "release": "yarn test && standard-version && npm publish && git push --follow-tags",
    "test": "yarn lint && mocha ./test/*.test.*"
  },
  "dependencies": {
    "ansi-escapes": "^5.0.0",
    "chalk": "^4.1.0",
    "consola": "^2.15.3",
    "figures": "^4.0.0",
    "markdown-table": "^3.0.1",
    "pretty-time": "^1.1.0",
    "std-env": "^3.0.0",
    "wrap-ansi": "^8.0.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/mocha": "^9.0.0",
    "@types/node": "latest",
    "codecov": "latest",
    "eslint": "latest",
    "jiti": "latest",
    "memory-fs": "latest",
    "mocha": "latest",
    "standard-version": "latest",
    "typescript": "latest",
    "unbuild": "latest",
    "webpack": "latest",
    "webpack-cli": "latest"
  },
  "peerDependencies": {
    "webpack": "3 || 4 || 5"
  },
  "engines": {
    "node": ">=14"
  }
}
